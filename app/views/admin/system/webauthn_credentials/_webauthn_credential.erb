<tr>
  <td><%= link_to webauthn_credential.user.name, admin_system_user_path(webauthn_credential.user) %></td>
  <td><%= webauthn_credential.authenticator_type %>
  <td><%= webauthn_credential.credential_id.truncate(27) %>
  <td><%= webauthn_credential.rp_id %></td>
  <td><%= webauthn_credential.nickname.presence || "-" %></td>
  <td><%= webauthn_credential.last_used ? systime(webauthn_credential.last_used) : "- never -" %></td>
  <td><%= systime(webauthn_credential.created_at) %></td>
  <td><%= systime(webauthn_credential.updated_at) %></td>
  <td class="actions">
    <%= link_to "edit", edit_admin_system_webauthn_credential_path(webauthn_credential) %> &nbsp;
    <%= link_to "delete",
                {action: "destroy", id: webauthn_credential.id},
                method: :delete,
                data: { confirm: "Are you sure you want to delete this passkey?" } %>
  </td>